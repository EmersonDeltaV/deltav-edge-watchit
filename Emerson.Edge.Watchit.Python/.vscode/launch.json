{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--provider-prefix", "example_prefix",
                "--path", "example_path",
                "--edgeip", "192.168.1.1",
                "--username", "admin",
                "--password", "secret"
            ]
        }
    ]
}